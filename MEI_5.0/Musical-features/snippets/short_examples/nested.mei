  <music>
    <body>
      <mdiv>
        <score>
          <scoreDef meter.count="3" meter.unit="4" keysig="1s">
            <staffGrp>
              <staffDef n="1" xml:id="P1" lines="5" clef.line="4" clef.shape="F" keysig="1s">
                <label>Part_1</label>
              </staffDef>
            </staffGrp>
          </scoreDef>
          <section>
            <scoreDef meter.count="3" meter.unit="4" keysig="1s"/>
            <measure n="1" xml:id="d1e12">
              <staff n="1">
                <layer n="1">
                  <app>
                    <!-- 2 possibilities for representing tuplets -->
                    <rdg>
                      <!-- notes followed by tupletspan -->
                      <beam>
                        <note xml:id="d1e34" pname="g" oct="3" dur="8" stem.dir="down"/>
                        <note xml:id="d1e51" pname="f" oct="3" dur="32" tuplet="i1" stem.dir="down" accid.ges="s"/>
                        <note xml:id="d1e80" pname="e" oct="3" dur="32" tuplet="m1" stem.dir="down"/>
                        <note xml:id="d1e106" pname="f" oct="3" dur="32" tuplet="m1" stem.dir="down" accid.ges="s"/>
                        <note xml:id="d1e134" pname="e" oct="3" dur="32" tuplet="m1" stem.dir="down"/>
                        <note xml:id="d1e160" pname="f" oct="3" dur="32" tuplet="t1" stem.dir="down" accid.ges="s"/>
                        <note xml:id="d1e189" pname="e" oct="3" dur="8" stem.dir="down"/>
                      </beam>
                      <beam>
                        <note xml:id="d1e206" grace="unacc" pname="c" oct="3" dur="16" stem.dir="up"/>
                        <note xml:id="d1e224" grace="unacc" pname="d" oct="3" dur="16" stem.dir="up"/>
                        <note xml:id="d1e242" grace="unacc" pname="e" oct="3" dur="16" stem.dir="up"/>
                      </beam>
                      <note xml:id="d1e261" pname="d" oct="3" dur="4" stem.dir="down"/>
                      <tupletSpan staff="1" startid="#d1e51" endid="#d1e160" num="5" numbase="2" num.visible="true" num.place="above" num.format="count"/>
                      <tupletSpan staff="1" startid="#d1e34" endid="#d1e189" num="3" numbase="2" num.visible="true" num.place="below" num.format="count"/>
                    </rdg>
                    <rdg>
                      <!-- nested tuplet elements -->
                      <tuplet num="3" numbase="2" num.visible="true" num.place="below" num.format="count">
                        <beam>
                          <note xml:id="d1e34.1" pname="g" oct="3" dur="8" stem.dir="down"/>
                          <tuplet num="5" numbase="2" num.visible="true" num.place="above" num.format="count">
                            <beam>
                              <note xml:id="d1e51.1" pname="f" oct="3" dur="32" tuplet="i1" stem.dir="down" accid.ges="s"/>
                              <note xml:id="d1e80.1" pname="e" oct="3" dur="32" tuplet="m1" stem.dir="down"/>
                              <note xml:id="d1e106.1" pname="f" oct="3" dur="32" tuplet="m1" stem.dir="down" accid.ges="s"/>
                              <note xml:id="d1e134.1" pname="e" oct="3" dur="32" tuplet="m1" stem.dir="down"/>
                              <note xml:id="d1e160.1" pname="f" oct="3" dur="32" tuplet="t1" stem.dir="down" accid.ges="s"/>
                            </beam>
                          </tuplet>
                          <note xml:id="d1e189.1" pname="e" oct="3" dur="8" stem.dir="down"/>
                        </beam>
                      </tuplet>
                    </rdg>
                  </app>
                </layer>
              </staff>
            </measure>
          </section>
        </score>
      </mdiv>
    </body>
