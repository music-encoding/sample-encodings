    <workList>
      <work xml:id="op89">
        <identifier>op.89</identifier>
        <title>Winterreise</title>
        <title type="uniform">Franz Schuberts Werke. Lieder und Gesänge</title>
        <creation>
          <date>1827 und 1828</date>
        </creation>
        <perfMedium>
          <perfResList>
            <perfRes codedval="va">Singstimme</perfRes>
            <perfRes codedval="ka">Pianoforte</perfRes>
          </perfResList>
        </perfMedium>
        <classification>
          <termList>
            <term>Liederzyklus</term>
          </termList>
        </classification>
        <componentList>
          <work n="1" xml:id="n1">
            <title>Gute Nacht</title>
            <composer>
              <persName role="composer">Franz Schubert</persName>
            </composer>
            <key accid="f" mode="minor">F Minor</key>
            <tempo>Mässig, in gehender Bewegung.</tempo>
            <meter count="2" unit="4"/>
            <incip>
              <incipCode form="parsons" label="Singstimme">*dddddududduddud</incipCode>
              <incipText>
                <p>Fremd bin ich eingezogen, fremd zieh´ ich wieder aus</p>
              </incipText>
            </incip>
            <history/>
            <perfMedium>
              <perfResList>
                <perfRes codedval="va">Singstimme</perfRes>
                <perfRes codedval="ka">Pianoforte</perfRes>
              </perfResList>
            </perfMedium>
          </work>
          <work n="2" xml:id="n2">
            <title>Die Wetterfahne</title>
            <composer>
              <persName role="composer">Franz Schubert</persName>
            </composer>
            <key mode="minor">a-moll</key>
            <tempo>Ziemlich geschwind, unruhig.</tempo>
            <meter count="6" unit="8"/>
            <incip>
              <incipText>
                <p>Der Wind spielt mit der Wetterfahne auf meines schönes Liebchens Haus.</p>
              </incipText>
              <incipCode form="parsons">*uddduuuddduuuddudduddu</incipCode>
            </incip>
            <perfMedium>
              <perfResList>
                <perfRes codedval="va">Singstimmme</perfRes>
                <perfRes codedval="ka">Pianoforte</perfRes>
              </perfResList>
            </perfMedium>
          </work>
          <work n="3" xml:id="n3">
            <title>Gefror´ne Thränen</title>
            <key pname="f" mode="minor">F Minor</key>
            <tempo>Nicht zu langsam</tempo>
            <meter count="2" unit="2" sym="cut"/>
            <incip>
              <incipCode form="parsons">*rddrduduudddrdu</incipCode>
              <incipText>
                <p>Gefror´ne Tropfen fallen von meinen Wangen ab: ob es mir denn entgangen, dass ich geweinet hab´?</p>
              </incipText>
            </incip>
          </work>
          <work n="4" xml:id="n4">
            <title>Erstarrung</title>
            <key pname="c" mode="minor">C minor</key>
            <tempo>Ziemlich schnell</tempo>
            <meter count="4" unit="4" sym="common"/>
            <incip>
              <incipCode form="parsons">*rrruudddruudu</incipCode>
              <incipText>
                <p>Ich such´ im Schnee vergebens nach ihrer Tritte Spur,</p>
              </incipText>
            </incip>
          </work>
          <work n="5" xml:id="n5">
            <title>Der Lindenbaum</title>
            <key pname="e" mode="major">E Major</key>
            <tempo>Mässig</tempo>
            <meter count="3" unit="4"/>
            <incip>
              <incipCode form="parsons">*rdrrrdruuuddd</incipCode>
              <incipText>
                <p>Am Brunnen vor dem Thore</p>
              </incipText>
            </incip>
            <langUsage>
              <language>german</language>
            </langUsage>
            <perfMedium>
              <perfResList>
                <perfRes codedval="va">Singstimme</perfRes>
                <perfRes codedval="ka">Pianoforte</perfRes>
              </perfResList>
            </perfMedium>
          </work>
          <work n="6" xml:id="n6">
            <title>Wasserfluth</title>
            <key pname="f" accid="s" mode="minor">F sharp Minor</key>
            <meter count="3" unit="4"/>
            <tempo>Langsam</tempo>
            <incip>
              <incipCode form="parsons">*uuuddddud</incipCode>
              <incipText>
                <p>Manche Thrän´ aus meinen Augen ist gefallen in den Schnee;</p>
              </incipText>
            </incip>
          </work>
          <work n="7" xml:id="n7">
            <title>Auf dem Flusse</title>
            <key pname="e" mode="minor">E Minor</key>
            <meter count="2" unit="4"/>
            <tempo>Langsam</tempo>
            <incip>
              <incipCode form="parsons">*ruuuur</incipCode>
              <incipText>
                <p>Der du so lustig rauschtest, du heller, wilder Fluss</p>
              </incipText>
            </incip>
          </work>
          <work n="8" xml:id="n8">
            <title>Rückblick</title>
            <key pname="g" mode="minor">G Minor</key>
            <tempo>Nicht zu geschwind</tempo>
            <meter count="3" unit="4"/>
            <incip>
              <incipCode form="parsons">*ruuuudduruuuuudd</incipCode>
              <incipText>
                <p>Es brennt mir unter beiden Sohlen, tret ich auch schon auf dem Eis und Schnee</p>
              </incipText>
            </incip>
          </work>
          <work n="9" xml:id="n9">
            <title>Irrlicht</title>
            <key pname="b" mode="minor">B Minor</key>
            <tempo>Langsam</tempo>
            <meter count="3" unit="8"/>
            <incip>
              <incipCode form="parsons">*rdrdudrurrrruuu</incipCode>
              <incipText>
                <p>In die tiefsten Felsengründe lockte mich ein Irrlicht hin</p>
              </incipText>
            </incip>
          </work>
          <work n="10" xml:id="n10">
            <title>Rast</title>
            <tempo>Mässig</tempo>
            <meter count="2" unit="4"/>
            <incip>
              <incipCode form="parsons">*rudruddrruruddud</incipCode>
              <incipText>
                <p>Nun merk´ ich erst, wie müd´ ich bin, da ich zur Ruh mich lege</p>
              </incipText>
            </incip>
            <expressionList>
              <expression n="10a">
                <title>Rast<titlePart type="subordinate">(Ursprüngliche Fassung)</titlePart></title>
                <key pname="d" mode="minor">D Minor</key>
              </expression>
              <expression n="10b">
                <title>Rast<titlePart type="subordinate">(Spätere Fassung)</titlePart></title>
                <key pname="c" mode="minor">C Minor</key>
              </expression>
            </expressionList>
          </work>
          <work n="11" xml:id="n11">
            <title>Frühlingstraum</title>
            <key pname="a" mode="major">A Major</key>
            <tempo>Etwas bewegt</tempo>
            <meter count="6" unit="8"/>
            <incip>
              <incipCode form="parsons">*rudurdrruuduudd</incipCode>
              <incipText>
                <p>Ich träumte von bunten Blumen, so wie sie wohl blühen im Mai</p>
              </incipText>
            </incip>
          </work>
          <work n="12" xml:id="n12">
            <title>Einsamkeit</title>
            <tempo>Langsam</tempo>
            <meter count="2" unit="4"/>
            <expressionList>
              <expression n="1">
                <title>Einsamkeit<titlePart type="subordinate">(Ursprüngliche Fassung)</titlePart></title>
                <key pname="d" mode="minor">D Minor</key>
                <tempo>Langsam</tempo>
                <incip>
                  <incipCode form="parsons">*rududuudrrdrruddu</incipCode>
                  <incipText>
                    <p>Wie eine trübe Wolke durch heit're Lüfte geht,</p>
                  </incipText>
                </incip>
              </expression>
              <expression n="2">
                <title>Einsamkeit<titlePart type="subordinate">(Spätere Fassung)</titlePart></title>
                <key pname="b" mode="minor">B Minor</key>
                <tempo>Langsam</tempo>
                <incip>
                  <incipCode form="parsons">*rududuudrrdrruddu</incipCode>
                  <incipText>
                    <p>Wie eine trübe Wolke durch heit're Lüfte geht,</p>
                  </incipText>
                </incip>
              </expression>
            </expressionList>
          </work>
          <work n="13" xml:id="n13">
            <title>Die Post</title>
            <key pname="e" accid="f" mode="major">E flat Major</key>
            <tempo>Etwas geschwind</tempo>
            <meter count="6" unit="8"/>
            <incip>
              <incipCode form="parsons">*rrrdrurd</incipCode>
              <incipText>
                <p>Von der Strasse her ein Posthorn klingt</p>
              </incipText>
            </incip>
          </work>
          <work n="14" xml:id="n14">
            <title>Der greise Kopf</title>
            <key pname="c" mode="minor">C Minor</key>
            <tempo>Etwas langsam</tempo>
            <meter count="3" unit="4"/>
            <incip>
              <incipCode form="parsons">*ruuuduudurdddddddd</incipCode>
              <incipText>
                <p>Der Reif hat einen weissen Schein mir über´s Haupt gestreuet</p>
              </incipText>
            </incip>
          </work>
          <work n="15" xml:id="n15">
            <title>Die Krähe</title>
            <key pname="c" mode="minor">C Minor</key>
            <tempo>Etwas langsam</tempo>
            <meter count="2" unit="4"/>
            <incip>
              <incipCode form="parsons">*dududdduddddd</incipCode>
              <incipText>
                <p>Eine Krähe war mit mir aus der Stadt gezogen</p>
              </incipText>
            </incip>
          </work>
          <work n="16" xml:id="n16">
            <title>Letzte Hoffnung</title>
            <key>Unclear</key>
            <tempo>Nicht zu geschwind</tempo>
            <meter count="3" unit="4"/>
            <incip>
              <incipCode form="parsons">*dudduudddduudd</incipCode>
              <incipText>
                <p>Hie und da ist an den Bäumen manches bunte Blatt zu seh´n</p>
              </incipText>
            </incip>
          </work>
          <work n="17" xml:id="n17">
            <title>IOm Dorfe</title>
            <key pname="d" mode="major">D Major</key>
            <tempo>Etwas langsam</tempo>
            <meter count="12" unit="8"/>
            <incip>
              <incipCode form="parsons">*rrrdrrurrdrr</incipCode>
              <incipText>
                <p>Es bleiben die Hunde, es rasseln die Ketten</p>
              </incipText>
            </incip>
          </work>
          <work n="18" xml:id="n18">
            <title>Der stürmische Morgen</title>
            <key pname="d" mode="minor">D Minor</key>
            <tempo>Ziemlich geschwind, doch kräftig</tempo>
            <meter count="4" unit="4" sym="common"/>
            <incip>
              <incipCode form="parsons">*ruduuuuduruduu</incipCode>
              <incipText>
                <p>Wie hat der Sturm zerrissen des Himmels graues Kleid!</p>
              </incipText>
            </incip>
          </work>
          <work n="19" xml:id="n19">
            <title>Täuschung</title>
            <key pname="a" mode="major">A Major</key>
            <tempo>Etwas geschwind</tempo>
            <meter count="6" unit="8"/>
            <incip>
              <incipCode form="parsons">*rrddrrdduud</incipCode>
              <incipText>
                <p>Ein Licht tanzt freundlich vor mir her</p>
              </incipText>
            </incip>
          </work>
          <work n="20" xml:id="n20">
            <title>Der Wegweiser</title>
            <key pname="g" mode="minor">G Minor</key>
            <tempo>Mässig</tempo>
            <meter count="2" unit="4"/>
            <incip>
              <incipCode form="parsons">*uurrrrduuudddu</incipCode>
              <incipText>
                <p>Was vermeid´ich denn die Wege, wo die andern Wandrer gehn</p>
              </incipText>
            </incip>
          </work>
          <work n="21" xml:id="n21">
            <title>Das Wirthshaus</title>
            <key pname="f" mode="major">F Major</key>
            <tempo>Ser langsam</tempo>
            <meter count="4" unit="4"/>
            <incip>
              <incipCode form="parsons">*urudrdrrrudu</incipCode>
              <incipText>
                <p>Auf einem Todtenacker hat mich mein Weg gebracht</p>
              </incipText>
            </incip>
          </work>
          <work n="22" xml:id="n22">
            <title>Muth</title>
            <key pname="a" mode="minor">A Minor</key>
            <tempo>Ziemlich geschwind, kräftig</tempo>
            <meter count="2" unit="4"/>
            <incip>
              <incipCode form="parsons">*uuudduduuddddud</incipCode>
              <incipText>
                <p>Fliegt der Schnee mir ins Gesicht, schüttl ihn herunter</p>
              </incipText>
            </incip>
          </work>
          <work n="23" xml:id="n23">
            <title>Die Nebensonnen</title>
            <key pname="a" mode="major">A Major</key>
            <tempo>Nicht zu langsam</tempo>
            <meter count="3" unit="4"/>
            <incip>
              <incipCode form="parsons">*rdrruudrrrddruurdd</incipCode>
              <incipText>
                <p>Drei Sonnen sah ich am Himmel stehn</p>
              </incipText>
            </incip>
          </work>
          <work n="24" xml:id="n24">
            <title>Der Leiermann</title>
            <tempo>Etwas langsam.</tempo>
            <meter count="3" unit="4"/>
            <incip>
              <incipCode form="parsons">*duuuddudud</incipCode>
              <incipText>
                <p>Drüben hinterm Dorfe steht ein Leiermann</p>
              </incipText>
            </incip>
            <expressionList>
              <expression n="1" xml:id="n24a">
                <title type="subordinate">Ursprüngliche Fassung</title>
                <key pname="b" mode="minor">B Minor</key>
              </expression>
              <expression n="2" xml:id="n24b">
                <title>Spätere Fassung</title>
                <key pname="a" mode="minor">A Minor</key>
              </expression>
            </expressionList>
          </work>
        </componentList>
      </work>
    </workList>
