          <section>
            <measure n="1" right="invis">
              <staff n="1">
                <layer n="1">
                  <!-- unbroken secondary beam -->
                  <beam>
                    <note pname="g" dur="16"/>
                    <note pname="g" dur="16"/>
                    <note pname="g" dur="16"/>
                    <note pname="g" dur="16"/>
                    <note pname="g" dur="16"/>
                    <note pname="g" dur="16"/>
